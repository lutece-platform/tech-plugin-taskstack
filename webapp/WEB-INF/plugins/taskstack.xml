<?xml version="1.0" encoding="UTF-8"?><plug-in>
    <name>taskstack</name>
    <class>fr.paris.lutece.plugins.taskstack.service.TaskStackPlugin</class>
    <version>1.0.3-SNAPSHOT</version>
    <documentation/>
    <installation/>
    <changes/>
    <user-guide/>
    <description>taskstack.plugin.description</description>
    <provider>taskstack.plugin.provider</provider>
    <provider-url>http://lutece.paris.fr</provider-url>
    <icon-url>images/admin/skin/feature_default_icon.png</icon-url>
    <copyright>Copyright (c) 2022 Mairie de Paris</copyright>
    <db-pool-required>1</db-pool-required>
    
    <core-version-dependency>
        <min-core-version>6.0.0</min-core-version>
        <max-core-version/>	
    </core-version-dependency>

    <!-- Admin features parameters -->
    <admin-features>
        <admin-feature>
            <feature-id>TASKSTACK_MANAGEMENT</feature-id>
            <feature-title>taskstack.title</feature-title>
            <feature-description>taskstack.plugin.description</feature-description>
            <feature-level>1</feature-level>
            <feature-url>jsp/admin/plugins/taskstack/TaskSearch.jsp</feature-url>
            <feature-icon-url/>
        </admin-feature>
    </admin-features>

    <!-- Daemons -->
    <daemons>
        <daemon>
            <daemon-id>purgeExpiredTasksDaemon</daemon-id>
            <daemon-name>taskstack.daemon.purgeExpiredTasksDaemon.name</daemon-name>
            <daemon-description>taskstack.daemon.purgeExpiredTasksDaemon.description</daemon-description>
            <daemon-class>fr.paris.lutece.plugins.taskstack.daemon.PurgeExpiredTasksDaemon</daemon-class>
        </daemon>
    </daemons>

    
</plug-in>